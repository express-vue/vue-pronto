{
  "name": "vue-pronto",
  "version": "1.5.2",
  "description": "Seriously fast vue server renderer",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js",
    ".babelrc"
  ],
  "dependencies": {
    "@types/deepmerge": "^2.1.0",
    "@types/object-hash": "^1.2.0",
    "browserify": "^16.1.1",
    "clean-css": "^4.1.11",
    "deepmerge": "^2.1.0",
    "find": "^0.2.9",
    "find-node-modules": "^1.0.4",
    "js-to-string": "^0.4.3",
    "lru-cache": "^4.1.2",
    "object-hash": "^1.3.0",
    "require-from-string": "^2.0.1",
    "uglify-js": "^3.3.16",
    "vue": "^2.5.16",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "vueify": "^9.4.1",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/clean-css": "^3.4.30",
    "@types/lru-cache": "^4.1.0",
    "@types/node": "^9.4.7",
    "@types/require-from-string": "^1.2.0",
    "@types/uglify-js": "^3.0.0",
    "@types/vue": "^2.0.0",
    "ava": "^0.25.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "express": "^4.16.3",
    "generate-release": "^0.14.0",
    "nodemon": "^1.17.2",
    "nsp": "^3.2.1",
    "nyc": "^11.6.0",
    "simple-vue-component-test": "^1.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "release": "npm test && generate-release",
    "debug": "nodemon --inspect tests/example/index.js",
    "start": "node tests/example/index.js",
    "debug-windows": "npm run build-windows && node --inspect tests/example/index.js",
    "start-windows": "npm run build-windows && node tests/example/index.js",
    "prepare": "nsp check",
    "lint": "tslint --project tsconfig.json",
    "pretest": "npm run lint",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "preversion": "npm test"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/example/**/*.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/assets/**/*"
    ],
    "reporter": [
      "text-summary"
    ]
  },
  "author": "Daniel Cherubini",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/express-vue/vue-pronto.git"
  },
  "keywords": [
    "vue",
    "express",
    "node",
    "ssr",
    "javascript",
    "stream",
    "io"
  ],
  "bugs": {
    "url": "https://github.com/express-vue/vue-pronto/issues"
  },
  "homepage": "https://github.com/express-vue/vue-pronto#readme"
}
