{
  "name": "vue-pronto",
  "version": "0.4.0",
  "description": "Seriously fast vue server renderer",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js"
  ],
  "dependencies": {
    "@types/deepmerge": "^1.3.3",
    "clean-css": "^4.1.9",
    "deepmerge": "^2.0.1",
    "insert-css": "^2.0.0",
    "lru-cache": "^4.1.1",
    "require-from-string": "^2.0.1",
    "uglify-js": "^3.3.7",
    "vue": "^2.5.13",
    "vue-server-renderer": "^2.5.13",
    "vue-template-compiler": "^2.5.13",
    "vueify": "^9.4.1",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/clean-css": "^3.4.30",
    "@types/lru-cache": "^4.1.0",
    "@types/node": "^9.3.0",
    "@types/require-from-string": "^1.2.0",
    "@types/uglify-js": "^2.6.30",
    "@types/vue": "^2.0.0",
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "express": "^4.16.2",
    "generate-release": "^0.14.0",
    "nodemon": "^1.14.12",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "vuetify": "^0.17.6"
  },
  "scripts": {
    "release": "generate-release",
    "debug": "nodemon --inspect tests/example/index.js",
    "start": "node tests/example/index.js",
    "debug-windows": "npm run build-windows && node --inspect tests/example/index.js",
    "start-windows": "npm run build-windows && node tests/example/index.js",
    "prepare": "nsp check",
    "lint": "tslint --project tsconfig.json",
    "pretest": "npm run lint",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "preversion": "npm test"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/example/**/*.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "lib/assets/**/*"
    ],
    "reporter": [
      "text-summary"
    ]
  },
  "author": "Daniel Cherubini",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/express-vue/vue-pronto.git"
  },
  "keywords": [
    "vue",
    "express",
    "node",
    "ssr",
    "javascript",
    "stream",
    "io"
  ],
  "bugs": {
    "url": "https://github.com/express-vue/vue-pronto/issues"
  },
  "homepage": "https://github.com/express-vue/vue-pronto#readme"
}
